{
    "name": "bartoszbartniczak/doctrine-datetime-with-microseconds",
    "description": "Doctine extension that adds microseconds to \\DateTime objects.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Bartosz Bartniczak",
            "email": "kontakt@bartoszbartniczak.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symplify/easy-coding-standard": "^7.2",
        "phpstan/phpstan": "^0.12.5",
        "vimeo/psalm": "^3.8",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "BartoszBartniczak\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BartoszBartniczak\\Doctrine\\": "tests/unit-tests"
        }
    }
}
